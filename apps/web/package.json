{
  "name": "astral-core-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:ws": "node server.js",
    "build": "pnpm exec next build",
    "build:next": "next build && tsc --project tsconfig.server.json",
    "vercel-build": "node scripts/vercel-build.js",
    "start": "next start",
    "start:ws": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:all": "node scripts/run-all-tests.js",
    "test:crisis": "jest tests/crisis --coverage",
    "test:ai-therapy": "jest tests/ai-therapy --coverage",
    "test:api": "jest tests/api --coverage",
    "test:pages": "jest tests/pages --coverage",
    "test:integration": "jest tests/integration --coverage",
    "test:accessibility": "jest tests/accessibility --coverage",
    "test:security": "jest tests/performance-security --testNamePattern='Security' --verbose",
    "test:performance": "jest tests/performance-security --testNamePattern='Performance' --verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:crisis": "playwright test tests/e2e/comprehensive-user-journeys.spec.ts --grep 'Crisis'",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:report": "jest --coverage && open coverage/lcov-report/index.html",
    "perf:test": "lighthouse-ci autorun"
  },
  "dependencies": {
    "@astralcore/database": "workspace:*",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.1",
    "@sentry/nextjs": "^10.12.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@tanstack/react-query": "^5.87.4",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.543.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^2.15.4",
    "redis": "^5.8.2",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "twilio": "^5.9.0",
    "typescript": "^5.7.2",
    "web-vitals": "^3.5.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.41.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "bcryptjs": "^3.0.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "loadtest": "^8.0.9",
    "playwright": "^1.41.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "socket.io-client": "^4.8.1",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "SKIP_ENV_VALIDATION": "true",
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}
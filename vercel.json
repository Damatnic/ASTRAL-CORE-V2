{
  "version": 2,
  "installCommand": "npm install -g pnpm@10.13.1 && pnpm install --frozen-lockfile",
  "buildCommand": "pnpm --filter astral-core-web build",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "env": {
    "SKIP_ENV_VALIDATION": "true",
    "SKIP_TYPE_CHECK": "true",
    "NODE_ENV": "production",
    "VERCEL": "1"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "true",
      "SKIP_TYPE_CHECK": "true"
    }
  }
}
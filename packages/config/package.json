{
  "name": "@astralcore/config",
  "version": "2.0.0",
  "description": "ASTRAL_CORE 2.0 Environment Configuration and Validation System",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "require": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
